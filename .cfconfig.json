{
	"cacheDefaultObject":"coldbox",
    "debuggingEnabled":true,
    "debuggingReportExecutionTimes":false,
    "disableInternalCFJavaComponents":false,
	"requestTimeoutEnabled":true,
	"requestTimeout":"0,0,5,0",
	"robustExceptionEnabled":true,
    "caches":{
		"coldbox":{
			"storage":"true",
            "type":"RAM",
            "custom":{
				"timeToIdleSeconds":"1800",
                "timeToLiveSeconds":"3600"
            },
            "class":"lucee.runtime.cache.ram.RamCache",
            "readOnly":"false"
        }
    },
	"datasources" : {
		"TestDB":{
            "allowAlter":false,
            "allowCreate":false,
            "allowDelete":true,
            "allowDrop":false,
            "allowGrant":false,
            "allowInsert":true,
            "allowRevoke":false,
            "allowSelect":true,
            "allowStoredProcs":true,
            "allowUpdate":true,
            "blob":false,
            "blobBuffer":64000,
            "class":"com.mssql.jdbc.Driver",
            "clientApplicationName":false,
            "clientApplicationNamePrefix":"",
            "clientHostname":false,
            "clientUsername":false,
            "clob":false,
            "clobBuffer":64000,
            "connectionLimit":-1,
            "connectionTimeout":30,
            "connectionTimeoutInterval":60,
            "custom":"useSSL=false&useUnicode=true&characterEncoding=UTF-8&serverTimezone=UTC&useLegacyDatetimeCode=true",
            "database":"TestDB",
            "dbdriver":"MSSQL",
            "description":"",
            "disableAutogeneratedKeyRetrieval":false,
            "disableConnections":false,
            "dsn":"jdbc:mssql://${DB_HOST}:1433;databaseName=TestDB",
            "host":"${DB_HOST}",
            "linkedServers":true,
            "logActivity":false,
            "logActivityFile":"",
            "loginTimeout":30,
            "maintainConnections":true,
            "maxPooledStatements":100,
            "password":"${DB_PASSWORD}",
            "port":"1433",
            "queryTimeout":0,
            "sendStringParametersAsUnicode":false,
            "serviceName":"",
            "SID":"",
            "username":"${DB_USER}",
            "validate":false,
            "validationQuery":""
        }
	},
	"whitespaceManagement":"white-space-pref"
}
